art (previousData)\r\n\r\n\r\n    }, [])\r\n\r\n    const  handleAddProduct =(product) =>{\r\n       const toBeAddedKey =product.key;\r\n       const sameProduct = cart.find(pd => pd.key === toBeAddedKey)\r\n       let count = 1;\r\n       let newCart ;\r\n       if(sameProduct){\r\n          count = sameProduct.quantity+ 1\r\n          sameProduct.quantity = count;\r\n          const others =cart.filter(pd => pd.key !== toBeAddedKey)\r\n          newCart = [...others,sameProduct]\r\n\r\n       }\r\n         else{\r\n             product.quantity = 1;\r\n             newCart = [...cart,product]\r\n         }\r\n        \r\n        setCart (newCart);\r\n\r\n        addToDatabaseCart(product.key ,count );\r\n    }\r\n\r\n    return (\r\n        <div className=\"shop-container\">\r\n\r\n            <div className=\"products-container\">\r\n\r\n        \r\n          \r\n                {\r\n                    products.map(pd =><Product \r\n                        key = {pd.key}\r\n                        showAddToCart= {true}\r\n                         handleAddProduct={handleAddProduct}\r\n                        product={pd} >\r\n                            </Product>)\r\n                }\r\n            \r\n\r\n            </div>\r\n            <div className=\"cart-container\">\r\n\r\n             <Cart cart ={cart}>\r\n             <Link to=\"/review\">\r\n            <button className=\"main-button\">Review</button>\r\n            </Link>\r\n             </Cart>\r\n\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default Shop;","C:\\WebDevelopment\\ema-john-simple\\src\\fakeData\\index.js",[],"C:\\WebDevelopment\\ema-john-simple\\src\\fakeData\\android.js",[],"C:\\WebDevelopment\\ema-john-simple\\src\\fakeData\\laptop.js",[],"C:\\WebDevelopment\\ema-john-simple\\src\\fakeData\\camera.js",[],"C:\\WebDevelopment\\ema-john-simple\\src\\components\\Product\\Product.js",["100"],"import React from 'react';\r\nimport { FontAwesomeIcon } from '@for
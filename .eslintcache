iv className=\"products-container\">\r\n\r\n        \r\n          \r\n                {\r\n                    products.map(pd =><Product \r\n                        key = {pd.key}\r\n                        showAddToCart= {true}\r\n                         handleAddProduct={handleAddProduct}\r\n                        product={pd} >\r\n                            </Product>)\r\n                }\r\n            \r\n\r\n            </div>\r\n            <div className=\"cart-container\">\r\n\r\n             <Cart cart ={cart}>\r\n             <Link to=\"/review\">\r\n            <button className=\"main-button\">Review</button>\r\n            </Link>\r\n             </Cart>\r\n\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default Shop;","C:\\WebDevelopment\\ema-john-simple\\src\\fakeData\\index.js",[],"C:\\WebDevelopment\\ema-john-simple\\src\\fakeData\\android.js",[],"C:\\WebDevelopment\\ema-john-simple\\src\\fakeData\\laptop.js",[],["98","99"],"C:\\WebDevelopment\\ema-john-simple\\src\\fakeData\\camera.js",[],"C:\\WebDevelopment\\ema-john-simple\\src\\components\\Product\\Product.js",["100"],"import React from 'react';\r\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome'\r\nimport { faCoffee , faShoppingCart} from '@fortawesome/free-solid-svg-icons'\r\nimport './Product.css';\r\nimport { Link } from 'react-router-dom';\r\n\r\nconst Product = (props) => {\r\n    //console.log(props);\r\n    const {name,img ,seller,price,stock,key} = props.product;\r\n    return (\r\n        <div className=\"productStyle\">\r\n            <div>\r\n         <img src={img} alt=\"\"/>   \r\n            </div>\r\n            <div>\r\n\r\n            <h4 className=\"productName\"><Link to={\"/product/\"+key}>{name}</Link></h4>\r\n            <br/>\r\n            <p><small> by : {seller}</small></p>\r\n            <br/>\r\n            <p> $ {price}</p>\r\n            <p><small>only {stock} lefts in stock .Order soon!!</small></p>\r\n            {prop